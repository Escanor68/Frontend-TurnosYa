  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "lighthouse": "lighthouse http://localhost:4000 --output=json --output-path=./lighthouse-report.json --chrome-flags=\"--headless --no-sandbox --disable-dev-shm-usage\"",
    "lighthouse:html": "lighthouse http://localhost:4000 --output=html --output-path=./lighthouse-report.html --chrome-flags=\"--headless --no-sandbox --disable-dev-shm-usage\"",
    "lhci:collect": "lhci collect",
    "lhci:assert": "lhci assert",
    "lhci:upload": "lhci upload",
    "analyze": "npm run build && npm run lighthouse:html",
    "performance": "node scripts/performance.js",
    "performance:full": "npm run build && npm run performance",
    "bundle:analyze": "npm run build && npx vite-bundle-analyzer dist",
    "preview:performance": "npm run build && npm run preview & sleep 5 && npm run lighthouse"
  },
